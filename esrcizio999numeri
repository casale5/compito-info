    <p id="output1"></p>
    
    
   <script>
function Numeri999Casuali(n) {
let dado=0;
let casuale=0;
let str="";
for(let i=0;i<n;i++)
{
casuale=Math.random();
dado=Math.round(casuale*998)+1;
str=str+" "+ dado;
}
document.getElementById("output1").innerHTML=str;
}

